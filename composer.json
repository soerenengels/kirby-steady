{
  "name": "soerenengels/kirby-steady",
  "type": "kirby-plugin",
  "description": "Steady meets Kirby is a Kirby Plugin.",
  "license": "MIT",
	"homepage": "https://kirby-steady.soerenengels.de/",
  "authors": [
    {
      "name": "SÃ¶ren Engels",
      "email": "mail@soerenengels.de"
    }
  ],
  "keywords": [
    "kirby5",
    "kirby5-cms",
    "kirby5-plugin",
    "steady"
  ],
  "require": {
		"php": ">=8.2.0",
    "getkirby/composer-installer": "^1.2.1"
  },
	"autoload": {
    "psr-4": {
      "Soerenengels\\": "classes/"
		}
  },
	"config": {
    "optimize-autoloader": true,
    "allow-plugins": {
      "getkirby/composer-installer": true
    }
  },
	"extra": {
		"installer-name": "steady",
    "kirby-cms-path": false
  },
  "require-dev": {
    "phpstan/phpstan": "^2.0",
    "getkirby/cms": "5.0.0-beta.1"
  }
}
